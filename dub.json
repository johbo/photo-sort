{
    "name": "photo-sort",
    "description": "Sorting photos based on time information.",
    "copyright": "Copyright Â© 2015, Johannes Bornhold",
    "authors": ["Johannes Bornhold"],
    "dependencies": {
        "docopt": ">=0.6.1-b.1",
        "freeimage": "~master",
        "logger": ">=0.3.3"
    },
    "configurations": [
        {
            "name": "default",
            "targetType": "executable",
            "targetName": "photo-sort"
